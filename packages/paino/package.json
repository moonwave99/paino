{
    "name": "@moonwave99/paino",
    "version": "0.1.14",
    "description": "Simple HTML/CSS Piano diagram generator",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "type": "module",
    "commonjs": "dist/paino.cjs",
    "types": "dist/index.d.ts",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "targets": {
        "commonjs": {
            "isLibrary": true,
            "outputFormat": "commonjs"
        }
    },
    "files": [
        "dist/paino.*.js",
        "dist/paino.*.map",
        "dist/**/*.d.ts",
        "src/styles/paino.css"
    ],
    "scripts": {
        "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
        "watch": "parcel watch",
        "build": "parcel build",
        "test": "npx ava",
        "examples": "webpack-dev-server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moonwave99/paino.git"
    },
    "keywords": [
        "music",
        "piano",
        "chords"
    ],
    "author": "Diego Caponera <hello@diegocaponera.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/moonwave99/paino/issues"
    },
    "homepage": "https://github.com/moonwave99/paino#readme",
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--loader=ts-node/esm",
            "--experimental-specifier-resolution=node"
        ]
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.6.0",
        "@parcel/transformer-typescript-types": "^2.6.0",
        "@types/node": "^17.0.36",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "ava": "^4.2.0",
        "browser-env": "^3.3.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.16.0",
        "esm": "^3.2.25",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "parcel": "^2.7.0",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1"
    },
    "dependencies": {
        "@tonaljs/midi": "^4.6.5",
        "@tonaljs/note": "^4.6.5",
        "change-case": "^4.1.2"
    },
    "gitHead": "6827e64cf890e66015d5fcb1dba9f87dd5012b4b"
}
