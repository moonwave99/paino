{
    "name": "@moonwave99/paino",
    "version": "0.0.7",
    "description": "Simple HTML/CSS Piano diagram generator",
    "main": "dist/paino.cjs.js",
    "module": "dist/paino.esm.js",
    "browser": "dist/paino.umd.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist/paino.*.js",
        "dist/paino.*.map",
        "dist/**/*.d.ts",
        "src/styles/paino.css"
    ],
    "scripts": {
        "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
        "start:dev": "npx rollup -c -w",
        "build:prod": "npx rollup -c",
        "test": "npx ava"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moonwave99/paino.git"
    },
    "keywords": [
        "music",
        "piano",
        "chords"
    ],
    "author": "Diego Caponera <hello@diegocaponera.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/moonwave99/paino/issues"
    },
    "homepage": "https://github.com/moonwave99/paino#readme",
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--loader=ts-node/esm",
            "--experimental-specifier-resolution=node"
        ]
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@types/node": "^17.0.36",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "ava": "^4.2.0",
        "browser-env": "^3.3.0",
        "eslint": "^8.16.0",
        "esm": "^3.2.25",
        "rollup": "^2.75.4",
        "rollup-plugin-typescript2": "^0.31.2",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    }
}
